			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        super();
        AdmSysAdminLogHelper helper = new AdmSysAdminLogHelper();
        int dateOffset = helper.GetDefaultNumDaysForReports();

        var currDateTime = DateTimeUtil::getSystemDateTime();
        date startDate = DateTimeUtil::date(DateTimeUtil::addDays(currDateTime, dateOffset));
        date endDate = DateTimeUtil::date(DateTimeUtil::addDays(currDateTime, 1));
        StartDateControl.dateValue(startDate);
        EndDateControl.dateValue(endDate);
    }

]]></Source>
			</Method>
