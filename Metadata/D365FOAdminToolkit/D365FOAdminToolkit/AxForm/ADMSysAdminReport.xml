						<Name>init</Name>
						<Source><![CDATA[
        public void init()
        {
            super();
            qry = this.query();
            ds = qry.dataSourceTable(tableNum(ADMSysAdminLog));
            ds.addSortField(fieldNum(ADMSysAdminLog, CreatedDateTime), SortOrder::Descending);
            qbr_date = ds.addRange(fieldNum(ADMSysAdminLog, CreatedDateTime));
        }

]]></Source>
					</Method>
					<Method>
						<Name>executeQuery</Name>
						<Source><![CDATA[
        public void executeQuery()
        {
            var startDateUtc = DateTimeUtil::newDateTime(StartDateControl.dateValue(), 0);
            var endDateUtc = DateTimeUtil::addDays(DateTimeUtil::newDateTime(EndDateControl.dateValue(), 0), 1);
            qbr_date.value(SysQuery::range(startDateUtc, endDateUtc));
            qryRun = new QueryRun(qry);

            super();
        }

]]></Source>
